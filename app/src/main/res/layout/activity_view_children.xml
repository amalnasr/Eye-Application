<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".viewChildren">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar1School"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginBottom="129dp"
        android:background="@drawable/toolscreen"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="studentname"
        app:titleMarginStart="150dp"
        app:titleMarginBottom="120dp"
        app:titleTextColor="#FAFFFF"> </androidx.appcompat.widget.Toolbar>



    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="150dp"

        app:srcCompat="@drawable/studentParent" />
    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="350dp"

        app:srcCompat="@drawable/evalouation" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="266dp"
        android:layout_marginTop="460dp"
        android:text="التقييم اليومي"
        android:textColor="#FFC107"
        android:textSize="15sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="120dp"
        android:layout_height="188dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="300dp"
        android:layout_marginRight="100dp"
        android:onClick="chDuti"
        app:srcCompat="@drawable/homework" />

    <TextView
        android:id="@+id/textViewHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="460dp"
        android:text=" الواجبات"
        android:textColor="#FFC107"
        android:textSize="15sp"
        android:textStyle="bold" />
</RelativeLayout>